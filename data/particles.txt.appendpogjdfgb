
sidesplash {
    #system properties
    movieclip soliddrip
    max_particles 800
    layer 2
    livebounds [-10, 64, -10, 42]
    batched true
    
    #emitter properties
    emit_rate [10,13]
    emit_amount [2,3]
    emit_direction [1, 0.5]
    emit_spread 90
    emit_box [1, 0]
    emit_offset [0,-.5]
    
    #particle animation properties
    particle_lifetime [.5,1.5]
    initial_speed [7,11]
    initial_rotation 0
    rotation_speed 0
    force [0, -20]
    friction 1
    size_start [.1,.4]
    size_end 0
    alpha_start 1
    alpha_end 1
    face_moving_direction true
    speed_scale [1,2]
    inherit_speed 1
    tags [collide_water collide_tile]
}


